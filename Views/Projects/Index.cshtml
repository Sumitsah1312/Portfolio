
@{
    ViewData["Title"] = "Projects";
     Layout = "~/views/Shared/_Layout.cshtml";
}
<div class="page-section container pt-5 mt-5">
    <h2>Projects</h2>
    <div class="row">
        @foreach(var p in Model)
        {
            <div class="col-md-4">
                <div class="card mb-3">
                    <div class="card-body">
                        <h5>@p.Title</h5>
                        <p>@p.Description</p>
                    </div>
                </div>
            </div>
        }
    </div>
</div>
